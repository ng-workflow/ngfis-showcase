{
  "name": "ngfis-showcase",
  "private": true,
  "dependencies": {
    "angular": "~1.3.7",
    "angular-touch": "~1.3.7",
    "bootstrap": "~3.3.1",
    "zepto": "~1.1.6"
  },
  "devDependencies": {
    "angular-mocks": "~1.3.7"
  },
  "ignoreDependencies": [
    "jquery"
  ],
  "overrides": {
    "bootstrap": {
      "main": "js/bootstrap.js",
      "mapping": [
        {
          "cwd": "dist",
          "src": [
            "css/**.css",
            "js/bootstrap.js",
            "fonts/**",
            "!**/*.min.*",
            "!**/*theme.*"
          ]
        }
      ]
    }
  }
}
